  /*MAX*/
  @media (max-width: 1200px) {
    header {
      padding: 8px;
      -webkit-box-shadow: 2px 2px 7px 0 rgba(0, 0, 0, 0);
      box-shadow: 2px 2px 7px 0 rgba(0, 0, 0, 0);
    }
    header a.logo {
      height: 30px;
      width: 200px;
    }
    .warp {
      background: #fff;
      margin-top: 40px;
      padding: 0 40px;
    }
    h2.main-title {
      text-align: left;
      font-size: 2.5rem;
      padding-bottom: 7px;
    }
    h2.main-title:before {
      display: none;
    }
  }
  @media (max-width: 992px) {
    ul.intro-list {
      display: block;
    }
    ul.intro-list li {
      display: block;
    }
    ul.intro-list li a .gap-1,
    ul.intro-list li a .gap-2,
    ul.intro-list li a .gap-3 {
      height: 15px !important;
    }
  }
  /*max-width: 992px*/
  /*max-width: 768px*/
  @media (max-width: 767px) {
    .warp {
      padding: 0 10px;
    }
  }
