/*theme green*/
/*@main-color-mid:@green-mid;*/
* {
    padding: 0;
    margin: 0;
    outline: 0;
  }
  
  *,
  ::before,
  ::after {
    box-sizing: border-box;
  }
  
  html {
    font-size: 62.5%;
    min-height: 100vh;
    position: relative;
    z-index: 1;
  }
  
  body {
    font-family: "微軟正黑體", "Microsoft JhengHei", "Segoe UI Semibold", "Segoe UI", "Lucida Grande";
    font-family: Helvetica, Arial, 'STHeiti', "Microsoft JhengHei", sans-serif !important;
    z-index: 2;
    image-rendering: -webkit-optimize-contrast;
    -ms-overflow-style: scrollbar;
    background: transparent;
    font-size: 1.6rem;
    position: relative;
    margin: 0 auto;
    overflow-x: hidden;
  }
  
  body ::selection {
    background: #e4004d;
    color: #fff
  }
  
  body ::-moz-selection {
    background: #e4004d;
    color: #fff
  }
  
  img {
    border: 0
  }
  
  ul,
  ol {
    margin: 0;
    list-style: none
  }
  
  a {
    text-decoration: none;
    display: inline-block
  }
  
  a.inline {
    display: inline;
    padding: 0 3px
  }
  
  a:hover,
  a:focus {
    color: #fe9150
  }
  
  a:focus {
    outline: 2px solid #0000FF !important;
  }
  
  a {
    text-decoration: none
  }
  
  a.sr-only {
    padding: 0.5rem;
    background: #2B358C;
    color: #FFF;
    position: absolute;
    top: -2.5rem;
    left: 0;
    -webkit-transition: top 1s ease-out;
    -o-transition: top 1s ease-out;
    transition: top 1s ease-out;
    z-index: 100;
    width: 0;
    height: 0;
    overflow: hidden;
  }
  
  #AU[accesskey] {
    color: #2B358C;
    margin-left: -1.8rem;
  }
  
  #AU:focus {
    color: #FFF;
  }
  
  #AC[accesskey] {
    color: #F7F7F7;
    line-height: 2rem;
    margin-top: 0.313rem;
  }
  
  #AC:focus {
    color: #000;
  }
  
  #AF[accesskey] {
    color: #000;
    margin-left: -1.8rem;
  }
  
  #AF:focus {
    color: #FFF;
  }
  .font {
    font-family: 'Roboto-light', Helvetica, Arial, 'STHeiti', "微軟正黑體", sans-serif;
  }
  .inset-icon {
    font-weight: bolder;
    color: rgba(65, 138, 127, 0.4);
    text-shadow: 1px 4px 6px #ffffff, 0 0 0 #2fac60, 1px 4px 6px #faf2e5;
  }
  .flex {
    display: flex;
    align-items: center;
    justify-content: center;
  }
  .flex-item-w {
    max-width: 50%;
  }
  .flex-item-top {
    align-self: flex-start;
  }
  .flex-item-bottom {
    align-self: flex-end;
  }
  .flex-center {
    display: -webkit-flex;
    /* Safari */
    -webkit-align-items: center;
    /* Safari 7.0+ */
    display: flex;
    align-items: center;
    /*水平置中*/
    justify-content: center;
    -webkit-justify-content: center;
  }
  .flex-center-left {
    display: -webkit-flex;
    /* Safari */
    -webkit-align-items: center;
    /* Safari 7.0+ */
    display: flex;
    align-items: center;
    /*水平置左*/
    justify-content: left;
    -webkit-justify-content: left;
  }
